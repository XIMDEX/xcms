<div id="kupu-datedrawer"
     class="kupu-drawer kupu-datedrawer ximlink_manager">
    <div class="title_panel">
        <h3 i18n:translate="">{t}Seleccione fecha{/t}</h3>
    </div>
    <div class="kupu-panels">
        <div class="search_links">
            <label for="ximlink-search" i18n:translate="" class="title">{t}Date{/t}</label>
            <input type="text" class="datepicker"/>
        </div>
        
    </div>
    <div class="buttons">
        <button class="kupu-dialog-button save-button btn main_action" type="button" i18n:translate="">{t}Accept{/t}</button>
    
        <button class="kupu-dialog-button close-button btn" type="button" i18n:translate="">{t}Cancel{/t}</button>
    </div>
</div>